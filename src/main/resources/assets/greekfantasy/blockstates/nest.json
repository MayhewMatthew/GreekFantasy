{
  "multipart": [
    {
      "apply": {
        "model": "greekfantasy:block/nest_slab"
      }
    },
    {
      "when": {
        "north": "false"
      },
      "apply": {
        "model": "greekfantasy:block/nest_quad",
        "uvlock": true
      }
    },
    {
      "when": {
        "east": "false"
      },
      "apply": {
        "model": "greekfantasy:block/nest_quad",
        "y": 90,
        "uvlock": true
      }
    },
    {
      "when": {
        "south": "false"
      },
      "apply": {
        "model": "greekfantasy:block/nest_quad",
        "y": 180,
        "uvlock": true
      }
    },
    {
      "when": {
        "west": "false"
      },
      "apply": {
        "model": "greekfantasy:block/nest_quad",
        "y": 270,
        "uvlock": true
      }
    }
  ]
}